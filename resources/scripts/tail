
//RULES ARE DEFINED HERE BY FINAL USER
}

Goals {
	goRight() :- isIn(X,Y) & isNext(X,Z)     & moveFromTo(X,Y,Z,Y).
	goLeft()  :- isIn(X,Y) & isPrevious(X,Z) & moveFromTo(X,Y,Z,Y).
	goUp()    :- isIn(X,Y) & isNext(Y,Z)     & moveFromTo(X,Y,X,Z).
	goDown()  :- isIn(X,Y) & isPrevious(Y,Z) & moveFromTo(X,Y,X,Z).
}

Events {

}
